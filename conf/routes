# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Calendar Controller
GET     /calendar/list              controllers.Calendar.CalendarController.List
POST    /calendar                   controllers.Calendar.CalendarController.Create
DELETE  /calendar/:id               controllers.Calendar.CalendarController.Delete(id: Int)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
