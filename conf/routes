# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Calendar Controller
GET     /calendars/list              controllers.CalendarController.List
POST    /calendars                   controllers.CalendarController.Create
DELETE  /calendars/:id               controllers.CalendarController.Delete(id: Int)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
